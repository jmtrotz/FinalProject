<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Hibernate mapping for the SE321 table
    Authors: Jeffrey Trotz & William Varner
    Class: CT406
    Date: 11/29/17
    Version: 1.0
-->
<hibernate-mapping>
    <class name="objectMapping.SE321" table="SE321" schema="APP" optimistic-lock="version">
        <id name="username" type="string">
            <column name="USERNAME"/>
            <generator class="assigned"/>
        </id>
        <property name="firstName" type="string">
            <column name="FIRSTNAME" length="20" not-null="true"/>
        </property>
        <property name="lastName" type="string">
            <column name="LASTNAME" length="20" not-null="true"/>
        </property>
        <property name="email" type="string">
            <column name="EMAIL" length="50" not-null="true"/>
        </property>
        <property name="assignment1" type="string">
            <column name="ASSIGNMENT1" length="20"/>
        </property>
        <property name="assignment2" type="string">
            <column name="ASSIGNMENT2" length="20"/>
        </property>
        <property name="assignment3" type="string">
            <column name="ASSIGNMENT3" length="20"/>
        </property>
        <property name="assignment4" type="string">
            <column name="ASSIGNMENT4" length="20"/>
        </property>
        <property name="assignment5" type="string">
            <column name="ASSIGNMENT5" length="20"/>
        </property>
        <property name="grade1" type="string">
            <column name="GRADE1" length="5"/>
        </property>
        <property name="grade2" type="string">
            <column name="GRADE2" length="5"/>
        </property>
        <property name="grade3" type="string">
            <column name="GRADE3" length="5"/>
        </property>
        <property name="grade4" type="string">
            <column name="GRADE4" length="5"/>
        </property>
        <property name="grade5" type="string">
            <column name="GRADE5" length="5"/>
        </property>
    </class>
</hibernate-mapping>